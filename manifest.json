{
    "manifest_version": 3,
    "name": "NamuSearcher",
    "version": "1.0",
    "description": "Add custom context menu items",
    "permissions": [
      "contextMenus",
      "activeTab",
      "scripting"
    ],
    "host_permissions": [
      "http://*/",
      "https://*/"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "48": "icon.png"
    },
    "action": {
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png"
      }
    }
  }